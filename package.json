{
  "name": "rs-designer",
  "description": "Rocksmith CDLC Designer",
  "author": "sandiz",
  "version": "0.0.1",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "add": "^2.0.6",
    "bootstrap": "^4.3.1",
    "easy-peasy": "^2.0.0",
    "electron-is-dev": "^1.0.1",
    "electron-localshortcut": "^3.1.0",
    "electron-prompt": "^1.2.0",
    "electron-window-state": "^5.0.1",
    "fs-extra": "^7.0.1",
    "localforage": "^1.7.3",
    "music-metadata": "^3.5.2",
    "node-id3": "^0.1.7",
    "prop-types": "^15.6.2",
    "react": "^16.4.1",
    "react-bootstrap": "^1.0.0-beta.5",
    "react-dom": "^16.5.2",
    "react-toastify": "^5.0.0",
    "request": "^2.34",
    "request-promise": "^4.2.2",
    "soundtouchjs": "https://github.com/sandiz/SoundTouchJS.git",
    "threads": "^0.12.1",
    "tmp": "^0.1.0",
    "webpack-dev-server": "^3.1.1",
    "whatwg-fetch": "^3.0.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-app-rewired start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "electron-builder install-app-deps && patch-package",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "contributors:check": "all-contributors check",
    "dist-mac": "yarn run build && yarn run electron-builder build --mac -c.extraMetadata.main=src/electron-starter.js -c.mac.identity=null",
    "dist-win": "yarn run build && yarn run electron-builder build --win -c.extraMetadata.main=src/electron-starter.js -c.mac.identity=null"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.7.2",
    "all-contributors-cli": "^5.4.0",
    "babel-preset-react-app": "^5.0.3",
    "ds-store": "^0.1.6",
    "electron": "^4.0.4",
    "electron-builder": "^20.39.0",
    "electron-installer-dmg": "^2.0.0",
    "electron-packager": "^12.2.0",
    "electron-winstaller": "^3.0.4",
    "eslint": "^5.6.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.14.0",
    "eslint-plugin-jsx-a11y": "^6.1.1",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.11.0",
    "foreman": "^3.0.1",
    "jest": "23.6.0",
    "jquery": "^3.4.0",
    "patch-package": "^5.1.1",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^1.6.2",
    "react-scripts": "^2.0.4"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "productName": "Rocksmith Designer",
    "appId": "com.sandiz.rsdesigner",
    "files": [
      "src/",
      ".",
      "!dev${/*}",
      "!samples${/*}",
      "!demos${/*}",
      "!doc${/*}",
      "!test${/*}",
      "!functest${/*}",
      "!src/lib/musicanalysis/build/*",
      "!src/lib/musicanalysis/cqt.npy",
      "!patches/*",
      "!packages/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "src/assets/icons/icon.icns",
      "target": [
        "dmg",
        "zip"
      ]
    },
    "win": {
      "icon": "src/assets/icons/icon.ico",
      "target": [
        "zip",
        "msi",
        "nsis"
      ]
    },
    "directories": {
      "buildResources": "src/assets/icons/",
      "output": "release-builds"
    },
    "asar": "false",
    "fileAssociations": [
      {
        "ext": "rsdirectory",
        "name": "RSDesigner Project Bundle",
        "icon": "icon.icns",
        "role": "editor",
        "isPackage": true
      },
      {
        "ext": "rsproject",
        "name": "RSDesigner Project",
        "icon": "icon.icns",
        "role": "editor",
        "isPackage": false
      }
    ]
  },
  "repository": "github:sandiz/rs-designer",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
