{
  "name": "bandish",
  "description": "Bandish: Transcribe and Analyse Music",
  "author": "sandiz",
  "version": "0.3.0",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.22.2",
    "@blueprintjs/icons": "^3.12.0",
    "@blueprintjs/select": "^3.11.2",
    "@simonwep/selection-js": "^1.4.1",
    "about-window": "^1.13.2",
    "album-art": "^2.0.1",
    "audiomotion-analyzer": "^1.1.0",
    "chord-chart": "^1.1.0",
    "chroma-js": "^2.1.0",
    "classnames": "^2.2.6",
    "colormap": "^2.3.1",
    "electron-devtools-installer": "^2.2.4",
    "electron-is-dev": "^1.0.1",
    "electron-localshortcut": "^3.2.1",
    "electron-window-state": "^5.0.1",
    "fs-extra": "^8.1.0",
    "isomorphic-git": "^0.72.0",
    "localforage": "^1.7.3",
    "lodash": "^4.17.15",
    "music-metadata": "^5.1.1",
    "next-frame": "^0.2.3",
    "normalize.css": "^8.0.1",
    "pixi.js": "^5.2.0",
    "prop-types": "^15.6.2",
    "psarcjs": "^0.1.4",
    "react": "^16.12.0",
    "react-beautiful-dnd": "^12.2.0",
    "react-dom": "^16.12.0",
    "react-hotkeys": "^2.0.0",
    "react-native-listener": "^1.1.0",
    "react-router-dom": "^5.1.2",
    "readline": "^1.3.0",
    "request": "^2.34",
    "request-promise": "^4.2.5",
    "soundtouchjs": "https://github.com/sandiz/SoundTouchJS.git",
    "teoria": "^2.5.0",
    "tmp": "^0.1.0",
    "tone": "^13.8.25",
    "typeface-inconsolata": "^0.0.72",
    "typeface-magra": "^0.0.71",
    "vexchords": "^1.2.0",
    "whatwg-fetch": "^3.0.0",
    "youtube-dl": "^2.2.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-app-rewired build",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-app-rewired start",
    "postinstall": "electron-builder install-app-deps && yarn patch-package",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "contributors:check": "all-contributors check",
    "build-ma": "node scripts/buildWasm.js && cd src/lib/musicanalysis && ./compile.sh",
    "build-ma-bench": "cd src/lib/musicanalysis && ./compile.sh --bench",
    "dist-mac-bench": "yarn run build && yarn run build-ma-bench && yarn run electron-builder build --publish=never --mac -c.extraMetadata.main=src/electron-starter.js -c.mac.identity=null",
    "dist-mac": "yarn run build && yarn run build-ma && yarn run electron-builder build --publish=never --mac -c.extraMetadata.main=src/electron-starter.js -c.mac.identity=null",
    "dist-win": "yarn run build && yarn run build-ma && yarn run electron-builder build --publish=never --win -c.extraMetadata.main=src/electron-starter.js"
  },
  "devDependencies": {
    "@babel/core": "^7.7.5",
    "@babel/plugin-proposal-optional-chaining": "^7.7.5",
    "@fortawesome/fontawesome-free": "^5.12.0",
    "@types/chroma-js": "^1.4.3",
    "@types/classnames": "^2.2.9",
    "@types/cross-spawn": "^6.0.1",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.23",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.17",
    "@types/pixi.js": "^5.0.0",
    "@types/react": "^16.9.16",
    "@types/react-dom": "^16.9.4",
    "@types/react-router-dom": "^5.1.3",
    "@types/simonwep__selection-js": "^1.2.0",
    "@types/tmp": "^0.1.0",
    "@types/wavesurfer.js": "^2.0.2",
    "@types/youtube-dl": "^1.12.2",
    "@typescript-eslint/eslint-plugin": "^2.11.0",
    "@typescript-eslint/parser": "^2.11.0",
    "all-contributors-cli": "^6.11.2",
    "babel-eslint": "^10.0.3",
    "babel-preset-react-app": "^9.1.0",
    "customize-cra": "^0.9.1",
    "ds-store": "^0.1.6",
    "electron": "^7.1.5",
    "electron-builder": "^21.2.0",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.1.1",
    "electron-winstaller": "^4.0.0",
    "eslint": "^6.7.2",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.7.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.19.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.17.0",
    "eslint-plugin-react-hooks": "^2.3.0",
    "foreman": "^3.0.1",
    "node-sass": "^4.13.0",
    "patch-package": "^6.2.0",
    "prettier": "^1.19.1",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^2.1.5",
    "react-scripts": "3.3.0",
    "typescript": "^3.7.3",
    "wasm-dce": "^1.0.2",
    "webpack": "^4.0.0",
    "webpack-dev-server": "^3.9.0",
    "worklet-loader": "^1.0.0"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "productName": "Bandish",
    "appId": "com.sandiz.rsdesigner",
    "files": [
      "src/",
      ".",
      "src/lib/musicanalysis/dist/*",
      "!dev${/*}",
      "!samples${/*}",
      "!demos${/*}",
      "!doc${/*}",
      "!test${/*}",
      "!functest${/*}",
      "!src/lib/musicanalysis/build/*",
      "!src/lib/musicanalysis/test/*",
      "!src/lib/musicanalysis/cqt.npy",
      "!patches/*",
      "!packages/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "src/assets/icons/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "electronLanguages": [
        "en"
      ]
    },
    "win": {
      "icon": "src/assets/icons/icon.ico",
      "target": [
        "zip",
        "msi",
        "nsis"
      ]
    },
    "directories": {
      "buildResources": "src/assets/icons/",
      "output": "release-builds"
    },
    "extraFiles": [
      {
        "from": "src/lib/musicanalysis/dist/",
        "to": "Resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asar": "true",
    "fileAssociations": [
      {
        "ext": "rsdbundle",
        "name": "RSDesigner Bundle",
        "icon": "icon.icns",
        "role": "editor",
        "isPackage": true
      },
      {
        "ext": "rsdproject",
        "name": "RSDesigner Project",
        "icon": "icon.icns",
        "role": "editor",
        "isPackage": false
      },
      {
        "ext": "mp3",
        "role": "viewer",
        "isPackage": false
      },
      {
        "ext": "wav",
        "role": "viewer",
        "isPackage": false
      }
    ]
  },
  "repository": "github:sandiz/rs-designer",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}