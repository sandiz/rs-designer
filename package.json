{
  "name": "rs-designer",
  "description": "Music Transcription & Rocksmith CDLC Designer",
  "author": "sandiz",
  "version": "0.2.1",
  "license": "GPL-3.0",
  "private": true,
  "dependencies": {
    "@blueprintjs/core": "^3.19.1",
    "@blueprintjs/icons": "^3.11.0",
    "@blueprintjs/select": "^3.11.1",
    "about-window": "^1.13.1",
    "album-art": "^2.0.1",
    "chord-chart": "^1.1.0",
    "classnames": "^2.2.6",
    "electron-is-dev": "^1.0.1",
    "electron-localshortcut": "^3.1.0",
    "electron-window-state": "^5.0.1",
    "fs-extra": "^8.1.0",
    "localforage": "^1.7.3",
    "music-metadata": "^4.8.4",
    "normalize.css": "^8.0.1",
    "pixi.js": "^5.2.0",
    "prop-types": "^15.6.2",
    "psarcjs": "^0.1.4",
    "react": "^16.11.0",
    "react-beautiful-dnd": "^12.0.0",
    "react-dom": "^16.11.0",
    "react-hotkeys": "^2.0.0",
    "react-native-listener": "^1.1.0",
    "readline": "^1.3.0",
    "request": "^2.34",
    "request-promise": "^4.2.5",
    "soundtouchjs": "https://github.com/sandiz/SoundTouchJS.git",
    "teoria": "^2.5.0",
    "tmp": "^0.1.0",
    "typeface-inconsolata": "^0.0.72",
    "typeface-magra": "^0.0.71",
    "vexchords": "^1.2.0",
    "whatwg-fetch": "^3.0.0",
    "youtube-dl": "^2.1.0"
  },
  "scripts": {
    "start": "nf start -p 3000",
    "build": "react-app-rewired build",
    "test": "react-app-rewired test --env=jsdom",
    "electron": "electron .",
    "electron-start": "node src/electron-wait-react",
    "react-start": "react-app-rewired start",
    "pack": "build --dir",
    "dist": "npm run build && build",
    "postinstall": "electron-builder install-app-deps && yarn patch-package",
    "contributors:add": "all-contributors add",
    "contributors:generate": "all-contributors generate",
    "contributors:check": "all-contributors check",
    "build-ma": "cd src/lib/musicanalysis && ./bench.sh --compile",
    "dist-mac": "yarn run build && yarn run build-ma && yarn run electron-builder build --publish=never --mac -c.extraMetadata.main=src/electron-starter.js -c.mac.identity=null",
    "dist-win": "yarn run build && yarn run build-ma && yarn run electron-builder build --publish=never --win -c.extraMetadata.main=src/electron-starter.js -c.mac.identity=null"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.11.2",
    "@types/classnames": "^2.2.9",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.0.22",
    "@types/node": "^12.12.7",
    "@types/react": "^16.9.11",
    "@types/react-dom": "^16.9.4",
    "@types/tmp": "^0.1.0",
    "@types/wavesurfer.js": "^2.0.2",
    "@typescript-eslint/eslint-plugin": "^2.6.1",
    "@typescript-eslint/parser": "^2.6.1",
    "all-contributors-cli": "^6.10.0",
    "babel-eslint": "^10.0.3",
    "babel-preset-react-app": "^9.0.2",
    "customize-cra": "^0.8.0",
    "ds-store": "^0.1.6",
    "electron": "^7.1.0",
    "electron-builder": "^22.1.0",
    "electron-devtools-installer": "^2.2.4",
    "electron-installer-dmg": "^3.0.0",
    "electron-packager": "^14.1.0",
    "electron-winstaller": "^4.0.0",
    "eslint": "^6.6.0",
    "eslint-config-airbnb": "^18.0.1",
    "eslint-config-prettier": "^6.5.0",
    "eslint-config-react": "^1.1.7",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-prettier": "^3.1.1",
    "eslint-plugin-react": "^7.16.0",
    "foreman": "^3.0.1",
    "node-sass": "^4.13.0",
    "patch-package": "^6.2.0",
    "prettier": "^1.18.2",
    "react-app-rewire-eslint": "^0.2.3",
    "react-app-rewired": "^2.1.4",
    "react-scripts": "3.2.0",
    "typescript": "^3.7.2",
    "webpack-dev-server": "^3.9.0",
    "worklet-loader": "^1.0.0"
  },
  "homepage": "./",
  "main": "src/electron-starter.js",
  "build": {
    "productName": "Meend",
    "appId": "com.sandiz.rsdesigner",
    "files": [
      "src/",
      ".",
      "src/lib/musicanalysis/dist/*",
      "!dev${/*}",
      "!samples${/*}",
      "!demos${/*}",
      "!doc${/*}",
      "!test${/*}",
      "!functest${/*}",
      "!src/lib/musicanalysis/build/*",
      "!src/lib/musicanalysis/cqt.npy",
      "!patches/*",
      "!packages/*"
    ],
    "dmg": {
      "contents": [
        {
          "x": 130,
          "y": 220
        },
        {
          "x": 410,
          "y": 220,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "mac": {
      "icon": "src/assets/icons/icon.icns",
      "target": [
        "dmg",
        "zip"
      ],
      "electronLanguages": [
        "en"
      ]
    },
    "win": {
      "icon": "src/assets/icons/icon.ico",
      "target": [
        "zip",
        "msi",
        "nsis"
      ]
    },
    "directories": {
      "buildResources": "src/assets/icons/",
      "output": "release-builds"
    },
    "extraFiles": [
      {
        "from": "src/lib/musicanalysis/dist/",
        "to": "Resources/bin",
        "filter": [
          "**/*"
        ]
      }
    ],
    "asar": "true",
    "fileAssociations": [
      {
        "ext": "rsdbundle",
        "name": "RSDesigner Bundle",
        "icon": "icon.icns",
        "role": "editor",
        "isPackage": true
      },
      {
        "ext": "rsdproject",
        "name": "RSDesigner Project",
        "icon": "icon.icns",
        "role": "editor",
        "isPackage": false
      },
      {
        "ext": "mp3",
        "role": "viewer",
        "isPackage": false
      },
      {
        "ext": "wav",
        "role": "viewer",
        "isPackage": false
      }
    ]
  },
  "repository": "github:sandiz/rs-designer",
  "browserslist": [
    ">0.2%",
    "not dead",
    "not ie <= 11",
    "not op_mini all"
  ]
}
